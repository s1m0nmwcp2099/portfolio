   "source": [
    "def del_div(thisStr):\n",
    "    if thisStr == 'E1':\n",
    "        thisStr = 'Champ'\n",
    "    return thisStr\n",
    "\n",
    "def add_amp(thisStr):\n",
    "    thisStr += 'amp'\n",
    "    return thisStr\n",
    "\n",
    "hdrs = df.columns.values.tolist()\n",
    "#type(hdrs)\n",
    "\n",
    "df.iloc[:,hdrs.index('Div')] = df.iloc[:,hdrs.index('Div')].apply([del_div, add_amp])\n",
    "# df.iloc[:,hdrs.index('Div')].apply([del_div, add_amp])\n",
    "df.head()"
   ]
